	"devDependencies": {
		"@companion-module/tools": "^2.6.1",
		"@types/debug": "^4.1.12",
		"@types/node": "^22.19.7",
		"eslint": "^9.39.2",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.8.1",
		"rimraf": "^6.1.2",
